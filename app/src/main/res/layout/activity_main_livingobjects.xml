<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:bind="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="liist"
            type="com.example.abeer.quarantine.viewmodel.livingobjects.ListLivingObjects">

        </variable>
        <variable
            name="detailItemDataLists"
            type="com.example.abeer.quarantine.viewmodel.ListItemDataDetail">

        </variable>
        <!--<variable-->
        <!--name="itemmplant"-->
        <!--type="com.example.abeer.quarantine.viewmodel.plantProduct.ItemData_PlantProduct">-->
        <!--</variable>-->
        <variable
            name="itemm"
            type="com.example.abeer.quarantine.viewmodel.ItemData">

        </variable>
        <variable
            name="clickliving"
            type="com.example.abeer.quarantine.presenter.ClicklivigDataObjects">

        </variable>

        <variable
            name="clicss"
            type="com.example.abeer.quarantine.presenter.ClickLotat">

        </variable>
        <variable
            name="detailItemData"
            type="com.example.abeer.quarantine.viewmodel.ListItemConstrains">
        </variable>
        <variable
            name="detailItemLotat5"
            type="com.example.abeer.quarantine.viewmodel.ListItemLotat">
        </variable>
        <!--<variable-->
        <!--name="adapteresh"-->
        <!--type="com.example.abeer.quarantine.adapter.AdapterEshtratatObjectliving">-->
        <!--</variable>-->
        <!--<variable-->
        <!--name="adapterlot"-->
        <!--type="com.example.abeer.quarantine.adapter.AdapterLotatLiving">-->

        <!--</variable>-->
        <variable
            name="adapteresh"
            type="com.example.abeer.quarantine.adapter.AdapterEshtratat">
        </variable>
        <variable
            name="adapterlot"
            type="com.example.abeer.quarantine.adapter.AdapterLotat">
        </variable>
    </data>

    <android.support.constraint.ConstraintLayout
        android:orientation="vertical"
        android:layoutDirection="rtl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/h2"
        android:layout_gravity="center"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context=".activity.MainActivity_subdetails"
        tools:showIn="@layout/app_bar_main_activity__listofchipment">

        <TextView
            android:id="@+id/title_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/droid"
            android:gravity="center"
            android:text="تفاصيل الكائنات الحية"
            android:textColor="#009540"
            android:textSize="50sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints"
            tools:textSize="30sp" />
        <Button
            android:id="@+id/shownavbar"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/ic_menu_black_24dp"
            android:layoutDirection="ltr"
            android:onClick="shownav"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.985"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.253" />

        <LinearLayout
            android:id="@+id/linearLayout13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:gravity="center"
            android:layoutDirection="rtl"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/title_list">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="غرض التصدير"
                    android:textColor="#1f90a5"
                    android:textSize="25dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="@{itemm.itemPurpose}"
                    android:textColor="#000"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="الحاله "
                    android:textColor="#1f90a5"
                    android:textSize="25dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="@{itemm.itemStatus}"
                    android:textColor="#000"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="الطور الحيوى"
                    android:textColor="#1f90a5"
                    android:textSize="25dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="@{itemm.item_Cat_Name}"
                    android:textColor="#000"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="السلالة"
                    android:textColor="#1f90a5"
                    android:textSize="25dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="@{itemm.item_Strain}"
                    android:textColor="#000"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text=" الاسم المختصر "
                    android:textColor="#1f90a5"
                    android:textSize="25dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="@{itemm.item_ShortName}"
                    android:textColor="#000"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="  الكود "
                    android:textColor="#1f90a5"
                    android:textSize="25dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="@{itemm.itemPartTypeName}"
                    android:textColor="#000"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </LinearLayout>



            <LinearLayout
                android:id="@+id/linearLayout12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout11">

                <TextView
                    android:id="@+id/constransliving"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/btnshadow"
                    android:fontFamily="@font/droid"
                    android:padding="10dp"
                    android:text="الاشتراطات"
                    android:onClick="ClickLiving"
                    android:textAlignment="center"
                    android:textColor="#fff"
                    android:textSize="20dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/lotssliving"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btnshadow"
                    android:fontFamily="@font/droid"
                    android:padding="10dp"
                    android:text="  بيانات اللوط "
                    android:onClick="ClickLiving"
                    android:textColor="#fff"
                    android:textSize="20dp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/Linearconstransliving"
            android:layout_width="518dp"
            android:layout_height="243dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/rectborderlots"
            android:gravity="center"
            android:layoutDirection="rtl"
            android:orientation="vertical"
            android:visibility="gone"
            android:padding="20dp"
            app:layout_constraintBottom_toTopOf="@+id/button2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout13"
            app:layout_constraintVertical_bias="0.16"
            tools:ignore="MissingConstraints">

            <android.support.v7.widget.RecyclerView
                android:id="@+id/detialsconstrans"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:adapter="@{adapteresh}"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.547"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.419" >
            </android.support.v7.widget.RecyclerView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/Linearlotssliving"
            android:layout_width="518dp"
            android:layout_height="243dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/rectborderlots"
            android:gravity="center"
            android:layoutDirection="rtl"
            android:orientation="vertical"
            android:visibility="gone"
            android:padding="20dp"
            app:layout_constraintBottom_toTopOf="@+id/button2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout13"
            app:layout_constraintVertical_bias="0.16"
            tools:ignore="MissingConstraints">
            <android.support.v7.widget.RecyclerView
                android:id="@+id/detialslotss"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:adapter="@{adapterlot}"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.547"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.419" >
            </android.support.v7.widget.RecyclerView>
        </LinearLayout>

    </android.support.constraint.ConstraintLayout>
</layout>